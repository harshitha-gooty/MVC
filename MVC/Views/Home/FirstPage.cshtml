@{
    ViewBag.Title = "FirstPage";
}

<h2>FirstPage</h2>
@*@{Html.RenderPartial("_Partial");}*@

<div class="form-group">
    <div class="form-group">
        @Html.Action("GetUserDetails", "UserDetails")
    </div>
    <div class="row">
        <div class="col-md-8">
            <p>Select users from above and login</p>
            <div class="form-group">
                @Html.Label("lblUserName", "UserName", new { @class = "col-md-2 control-label" })
                <div class="col-md-10">
                    @Html.TextBox("txtUserName", "", new { @class = "form-control", placeholder = "Enter Name" })
                </div>
            </div>
            <div class="form-group">
                @Html.Label("lblPassword", "Password", new { @class = "col-md-2 control-label" })
                <div class="col-md-10">
                    @Html.TextBox("txtPassword", "", new { @class = "form-control", placeholder = "Enter Password" })
                </div>
            </div>
            <div class="form-group ">
                <div class="col-md-10 col-md-offset-2">
                    <input type="submit" id="submit" value="Log in" class="btn btn-default" />
                </div>
            </div>
        </div>
    </div>
</div>

<script src="~/Scripts/Home/FirstPage.js"></script>



